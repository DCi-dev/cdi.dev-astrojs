---
import Hamburger from "./navbar/Hamburger.astro";
import Logo from "./navbar/Logo.astro";
import MobileNav from "./navbar/MobileNav.astro";
import Nav from "./navbar/Nav.astro";
import ThemeToggle from "./ThemeToggle.astro";
---
<script defer src="https://unpkg.com/alpinejs@3.x.x/dist/cdn.min.js"></script>


<header
x-data="{ isOpen: false }"
  class="fixed top-0 z-50 flex w-full items-center justify-between bg-neutral-100 px-4 py-[10px] text-neutral-1000 dark:bg-neutral-1000 dark:text-neutral-100 print:hidden sm:p-4 sm:px-6"
>
 <Hamburger/>
  <MobileNav />
  <Logo/>
  <Nav/>
  <ThemeToggle />
</header>
